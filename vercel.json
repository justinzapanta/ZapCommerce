{
    "version": 2,
    "builds": [
      {
        "src": "Zapcommerce/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9" 
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Zapcommerce/wsgi.py" 
      }
    ],
    "build": {
      "commands": [
        "python manage.py collectstatic --noinput" 
      ]
    }
  }